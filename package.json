{
  "name": "hoolinks",
  "version": "1.0.0",
  "description": "境案通官网项目",
  "author": "duanguang",
  "private": true,
  "scripts": {
    "dev": "NUXT_ENV_SERVER=dev nuxt",
    "dev:win": "cross-env NUXT_ENV_SERVER=dev nuxt",
    "build:qa": "NUXT_ENV_SERVER=qa nuxt build",
    "build:uat": "NUXT_ENV_SERVER=uat  nuxt build",
    "build:prod": "NUXT_ENV_SERVER=prod nuxt build",
    "build:demo": "NUXT_ENV_SERVER=demo nuxt build",
    "build-a": "nuxt build --analyze",
    "start": "nuxt start",
    "start:uat": "NUXT_PORT=8172 nuxt start",
    "start:demo": "NUXT_PORT=8173 nuxt start",
    "stop": "pm2 delete all",
    "pm2": "pm2 start npm --name my-nuxt -- run build",
    "pm2dev": "pm2 start ./node_modules/nuxt/bin/nuxt --name nuxt3 --env production",
    "generate": "NUXT_ENV_SERVER=uat nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "nuxt": "nuxt"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": 8171
    }
  },
  "dependencies": {
    "@nuxtjs/style-resources": "^0.1.2",
    "ant-design-vue": "^1.3.8",
    "axios": "^0.20.0",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "hoolinks": "^0.1.5",
    "iview": "^3.5.4",
    "json-mapper-object": "^0.0.9-rc1",
    "nuxt": "^2.5.2",
    "vue-awesome-swiper": "^3.1.3",
    "vue-class-component": "^6.3.2",
    "vue-count-to": "^1.0.13",
    "vue-image-crop-upload": "^2.5.0",
    "vue-quill-editor": "^3.0.6",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-import": "^1.8.0",
    "babel-preset-vue-app": "^2.0.0",
    "cross-env": "^2.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "jsdom": "^12.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.1",
    "pug-loader": "^2.4.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "style-loader": "^0.23.0"
  }
}
